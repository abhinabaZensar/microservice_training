</bean>

<bean id="item_5" class="com.spring.Item">
	<property name="name" value="samsung tv"/>
	<property name="value" value = "65000"/>
</bean>


<bean id="order_1" class="com.spring.Order" scope="prototype" >
	<property name="title" value="purchaseOrder_1"/>
</bean>
